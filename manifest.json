{
    "manifest_version": 2,
    "name": "YouTube Ad Skipper",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
    
}
